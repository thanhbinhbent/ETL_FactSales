                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
CAST(soh.OrderDate AS date) AS OrderDate,
    soh.DueDate, 
    soh.ShipDate, 
    soh.CustomerID,
   soh.TerritoryID ,
    sod.ProductID, 
    sod.OrderQty, 
